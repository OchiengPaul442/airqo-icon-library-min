{
  "buildCommand": "cd .. && pnpm build:website2",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "rewrites": [{ "source": "/(.*)", "destination": "/$1" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
    }
  ]
}

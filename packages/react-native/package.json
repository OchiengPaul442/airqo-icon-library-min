{
  "name": "@airqo-icons-min/react-native",
  "version": "1.2.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc -b && pnpm run build:esm",
    "build:esm": "esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/index.esm.js --external:react-native --sourcemap",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@airqo-icons-min/core": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^17 || ^18 || ^19",
    "react-native": "*",
    "react-native-svg": "*"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "esbuild": "^0.19.0",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "react-native-svg": "^15.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.2.2"
  }
}

{
  "name": "@airqo-icons/react-native",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.js --external:react --external:react-native --external:react-native-svg --sourcemap",
    "build:esm": "esbuild src/index.ts --bundle --platform=neutral --format=esm --outfile=dist/index.esm.js --external:react --external:react-native --external:react-native-svg --sourcemap",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@airqo-icons/core": "workspace:~",
    "react": "^17 || ^18",
    "react-native": "*",
    "react-native-svg": "*"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/react": "^18.3.20",
    "@types/react-native": "^0.72.5",
    "esbuild": "^0.19.0",
    "react": "^18.2.0",
    "react-native": "^0.79.2",
    "react-native-svg": "^13.9.0",
    "rimraf": "^5.0.10",
    "typescript": "^5.2.2"
  }
}

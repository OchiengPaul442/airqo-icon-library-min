{
  "name": "@airqo-icons-min/react",
  "version": "1.0.2",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "directory": "dist"
  },
  "scripts": {
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.js --external:react --sourcemap",
    "build:esm": "esbuild src/index.ts --bundle --platform=neutral --format=esm --outfile=dist/index.esm.js --external:react --sourcemap",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@airqo-icons-min/core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/react": "^18.3.20",
    "esbuild": "^0.19.0",
    "react": "^18.2.0",
    "rimraf": "^5.0.10",
    "tsup": "^8.4.0",
    "typescript": "^5.2.2"
  }
}

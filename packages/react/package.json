{
  "name": "@airqo-icons-min/react",
  "version": "1.0.2",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "client": [
        "client.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "client.js",
    "client.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.js --external:react --sourcemap",
    "build:esm": "esbuild src/index.ts --bundle --platform=neutral --format=esm --outfile=dist/index.esm.js --external:react --sourcemap",
    "build:main:cjs": "esbuild src/main.ts --bundle --platform=node --format=cjs --outfile=dist/main.js --external:react --sourcemap",
    "build:main:esm": "esbuild src/main.ts --bundle --platform=neutral --format=esm --outfile=dist/main.esm.js --external:react --sourcemap",
    "build:client:cjs": "esbuild src/client.tsx --bundle --platform=node --format=cjs --outfile=dist/client.js --external:react --sourcemap",
    "build:client:dev": "esbuild src/client.tsx --bundle --platform=node --format=cjs --outfile=dist/client.development.js --external:react --sourcemap",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "build": "npm run build:cjs && npm run build:esm && npm run build:main:cjs && npm run build:main:esm && npm run build:client:cjs && npm run build:client:dev && npm run build:types && npm run verify-types && cp src/client.tsx dist/client.src.tsx",
    "verify-types": "node scripts/verify-types.js",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@airqo-icons-min/core": "1.0.2"
  },
  "peerDependencies": {
    "react": "^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@types/react": "^18.3.20",
    "esbuild": "^0.19.0",
    "react": "^18.2.0",
    "rimraf": "^5.0.10",
    "typescript": "^5.2.2"
  }
}

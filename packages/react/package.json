{
  "name": "@airqo-icons-min/react",
  "version": "1.2.1",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "client": [
        "client.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./client.d.ts",
      "require": "./client.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "client.js",
    "client.d.ts",
    "client.development.js"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "rimraf dist && node scripts/ensure-dist.js",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.js --external:react --sourcemap",
    "build:esm": "esbuild src/index.ts --bundle --platform=neutral --format=esm --outfile=dist/index.esm.js --external:react --sourcemap",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "build": "pnpm run prebuild && pnpm run build:cjs && pnpm run build:esm && pnpm run build:types && node -e \"require('fs').copyFileSync('src/client.tsx', 'dist/client.src.tsx')\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@airqo-icons-min/core": "1.2.1"
  },
  "peerDependencies": {
    "react": "^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@types/react": "^18.3.20",
    "esbuild": "^0.19.0",
    "rimraf": "^5.0.10",
    "typescript": "^5.2.2"
  }
}

{
  "name": "@airqo-icons/core",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "rimraf dist && pnpm run generate-icons",
    "build": "tsc -b && pnpm run build:esm",
    "build:esm": "esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/index.esm.js --external:react --sourcemap",
    "clean": "rimraf dist",
    "generate-icons": "cd ../.. && node scripts/generate-icons.js",
    "prepare": "pnpm run build"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "csstype": "^3.1.3"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "esbuild": "^0.19.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.2.2"
  }
}

{
  "name": "@airqo-icons/core",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "pnpm run generate-icons",
    "build": "tsc src/icon-manifest.ts --module esnext --target es6 && tsc -b",
    "clean": "rm -rf dist",
    "generate-icons": "cd ../.. && node scripts/generate-icons.js",
    "prepare": "pnpm run build"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.20",
    "typescript": "^4.9.5"
  }
}

{
  "name": "@airqo-icons-min/icons",
  "version": "1.5.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "pnpm -r run clean",
    "generate": "node scripts/generate-icons.js",
    "enhance-rn": "node scripts/enhance-rn-icons.js",
    "optimize": "node scripts/optimize-svgs.js",
    "prebuild": "pnpm clean && pnpm generate && pnpm fix-component-types && pnpm enhance-rn",
    "build": "pnpm build:core && pnpm build:packages-no-vue",
    "build:core": "cd packages/core && npm run build",
    "build:packages-no-vue": "pnpm --filter \"./packages/react\" --filter=\"./packages/react-native\" build",
    "build:packages": "pnpm --filter \"./packages/react\" --filter=\"./packages/react-native\" --filter=\"./packages/vue\" build",
    "fix-component-types": "node scripts/fix-component-types.js",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "release": "pnpm build && node scripts/publish-packages.js",
    "changelog": "node scripts/generate-changelog.js"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^22.15.3",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.10.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "postcss": "^8.5.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-native": "^0.79.2",
    "react-native-svg": "^15.11.2",
    "rimraf": "^5.0.10",
    "semantic-release": "^21.0.7",
    "svgo": "^2.8.0",
    "tailwindcss": "^4.1.5",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "packageManager": "pnpm@8.10.0"
}
